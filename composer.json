{
    "name": "elegantmedia/laravel-simple-repository",
    "type": "library",
    "description": "Lightweight Repository pattern for Laravel 8 projects. Includes a repository generator.",
    "keywords": [
        "elegantmedia",
        "emedia",
        "laravel-simple-repository",
        "repository",
        "repositories",
        "pattern",
        "eloquent",
        "laravel8",
        "orm",
        "l5repository",
        "generator"
    ],
    "homepage": "https://github.com/elegantmedia/laravel-simple-repository",
    "license": "MIT",
    "authors": [
        {
            "name": "Elegant Media",
            "homepage": "http://www.elegantmedia.com.au",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/console": "^8.0|^9.0",
        "illuminate/database": "^8.0|^9.0",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "ElegantMedia\\SimpleRepository\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ElegantMedia\\SimpleRepository\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ElegantMedia\\SimpleRepository\\SimpleRepositoryServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
	"minimum-stability": "dev",
	"prefer-stable": true
}
