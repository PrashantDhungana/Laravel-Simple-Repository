{
    "name": "elegantmedia/laravel-simple-repository",
    "type": "library",
    "description": "Basic implementation of Repository pattern for Laravel projects.",
    "keywords": [
        "elegantmedia",
        "laravel-simple-repository",
        "repository",
        "pattern",
        "eloquent",
        "laravel8"
    ],
    "homepage": "https://github.com/elegantmedia/laravel-simple-repository",
    "license": "MIT",
    "authors": [
        {
            "name": "Elegant Media",
            "homepage": "http://www.elegantmedia.com.au",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "illuminate/console": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "ElegantMedia\\SimpleRepository\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ElegantMedia\\SimpleRepository\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ElegantMedia\\SimpleRepository\\SimpleRepositoryServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
